{
  "name": "app",
  "version": "1.0.0",
  "description": "The frontEnd application with React",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "build": "webpack-dev-server --config webpack/webpack.prod.js"
  },
  "author": "Daniel Ben-Lulu",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-class": "^1.0.0",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-config-strict-react": "^9.0.2",
    "eslint-plugin-react": "^7.17.0",
    "react-hot-loader": "^4.12.14",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-react"
    ],
    "plugins": [
      "react-html-attrs",
      "@babel/plugin-transform-async-to-generator",
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "regenerator-runtime": "^0.13.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "strict-react/es6"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ]
    },
    "globals": {
      "__dirname": "readonly",
      "document": "readonly",
      "module": "writable",
      "require": "readonly",
      "window": "writable"
    }
  }
}
