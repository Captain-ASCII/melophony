{
  "name": "app",
  "version": "2.0.0",
  "description": "The frontEnd application with React",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.dev.js",
    "minify": "cat $(find public/css -not -name '*App*' | grep -E '*\\.css') > full.tmp.css && lessc -clean-css full.tmp.css public/App.min.css && rm full.tmp.css",
    "bundle": "webpack -p --config webpack/webpack.prod.js",
    "build": "npm run minify && npm run bundle",
    "production": "sudo nodejs server.js"
  },
