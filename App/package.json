{
  "name": "app",
  "version": "1.0.0",
  "description": "The frontEnd application with React",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "build": "webpack -p --config webpack/webpack.prod.js",
    "production": "nodejs server.js"
  },
  "author": "Daniel Ben-Lulu",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-class": "^1.0.0",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.12.14",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-react"
    ],
    "plugins": [
      "react-html-attrs",
      "@babel/plugin-transform-async-to-generator",
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.3"
  }
}
