<div id="artists">
    {{#artists}}
        <div class="artistListItem" onclick="displayScreen('artist/{{id}}')">
            <div class="name">
                <p>{{name}}</p>
            </div>
            <div class="artistActions">
                <i onclick="displayScreen('artist/modify/{{id}}')" class="fa fa-pen icon"></i>
            </div>
        </div>
    {{/artists}}
</div>