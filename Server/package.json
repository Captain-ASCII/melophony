    "ws": "^7.1.2",
    "ytdl": "latest",
    "ytdl-core": "latest"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "babel-preset-es2015": "^6.24.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
