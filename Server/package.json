{
  "name": "melophony-server",
  "version": "1.0.0",
  "description": "Simple server to download from youtube before transferring to devices",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "dev": "nodemon index.js -e js -i nohup.out -V --exec babel-node --presets @babel/preset-env",
